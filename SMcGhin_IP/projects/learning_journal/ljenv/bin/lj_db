#!/Users/SMcGhin/Documents/python/SMcGhin_IP/projects/learning_journal/ljenv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'learning-journal','console_scripts','lj_db'
__requires__ = 'learning-journal'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('learning-journal', 'console_scripts', 'lj_db')()
    )
